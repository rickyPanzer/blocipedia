<%= simple_form_for [@user, @wiki], url: user_wikis_path, method: :post do |f| %>
  <%= f.input :title %>
  <%= f.input :body %>
  <% if current_user.role == "admin" || current_user.role == "premium" %>
    
    <%= f.input :private %>

    <%= f.input :collaborators, collection: @collaborators, as: :check_boxes  %>
    
  <% end %>
  <%= f.submit class: "btn btn-success"  %>
<% end %>